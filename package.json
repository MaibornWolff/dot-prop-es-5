    "delete",
    "del",
    "access",
    "notation",
    "dotty"
  ],
  "dependencies": {
    "is-obj": "^1.0.0"
  },
  "devDependencies": {
    "ava": "*",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "matcha": "^0.7.0",
    "xo": "*"
  },
  "xo": {
    "esnext": true
  }
}
